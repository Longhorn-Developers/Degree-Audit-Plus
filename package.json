    "dexie": "^4.2.1",
    "framer-motion": "^12.23.24",
    "json2csv": "^6.0.0-alpha.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.1.12",
